{
    include: [ "syslog/client.shard.cml" ],
    program: {
        runner: "elf",
        binary: "bin/stardock-manager",
    },
    capabilities: [
        { protocol: "fuchsia.stardock.Manager" },
    ],
    expose: [
        {
            protocol: "fuchsia.stardock.Manager",
            from: "self",
        },
    ],
}
